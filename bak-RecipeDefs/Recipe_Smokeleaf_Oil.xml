<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>SE_ExtractOil</defName>
		<label>extract smokeleaf oil</label>
		<description>Extract smokeleaf oil from a cooked smokeleaf plant.</description>
		
		<descriptionHyperlinks>
			<ThingDef>SE_CookedSmokeleaf</ThingDef>
		</descriptionHyperlinks>
		
		<jobString>Sauteing Smokeleaf</jobString>
		<workSkill>Cooking</workSkill>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<workAmount>300</workAmount>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Brewing</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		
		<recipeUsers>
		<li>ElectricStove</li>
		<li>FueledStove</li>
		<li>DrugLab</li>
		</recipeUsers>
		
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>SE_CookedSmokeleaf</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		
		<fixedIngredientFilter>			
			<thingDefs>
				<li>SE_CookedSmokeleaf</li>
				
			</thingDefs>
<!--		<categories>
				
			</categories> -->
		</fixedIngredientFilter>
		
		<defaultIngredientFilter>			
			<thingDefs>
				<li>SE_CookedSmokeleaf</li>
			</thingDefs>
	<!--	<categories>
				
			</categories> -->
		</defaultIngredientFilter>
		
		<products>
			<SE_SmokeleafOil>5</SE_SmokeleafOil>
		</products>
		
		<researchPrerequisite>SE_OilExtraction</researchPrerequisite>

		
	</RecipeDef>
</Defs>